function setup() {
    createCanvas(400, 400);
    background(220);
    // HSB with H range 0-400, S range 0-400, B range 0-100:
    colorMode(RGB, 255, 255, 255);
}

function draw() {
    let Han = random(width);
    let Solo = random(height);
    fill(Han, Solo, 100);
    noStroke()
    ellipse(Han, Solo, 20);
}
